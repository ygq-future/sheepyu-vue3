<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="top.sheepyu.module.system.dao.user.SystemUserMapper">
    <delete id="removeByUsernameDeleted">
        delete from system_user where username = #{username} and deleted = 1
    </delete>
</mapper>
